{
    "name": "clockwork-core",
    "version": "0.1.0",
    "description": "Core Module for Clockwork Bots",
    "scripts": {
        "start": "node --inspect=5858 -r ts-node/register ./src/index.ts",
        "start:watch": "nodemon",
        "build": "tsc"
    },
    "nodemonConfig": {
        "ignore": [
            "**/*.test.ts",
            "**/*.spec.ts",
            ".git",
            "node_modules"
        ],
        "watch": [
            "src"
        ],
        "exec": "npm start",
        "ext": "ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MNekrasov/clockwork-core.git"
    },
    "author": "Nekrasov",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/MNekrasov/clockwork-core/issues"
    },
    "homepage": "https://github.com/MNekrasov/clockwork-core#readme",
    "dependencies": {
        "@types/cheerio": "^0.22.17",
        "@types/moment-timezone": "^0.5.13",
        "@types/mongodb": "^3.5.4",
        "@typescript-eslint/eslint-plugin": "^2.25.0",
        "@typescript-eslint/parser": "^2.25.0",
        "axios": "^0.19.2",
        "cheerio": "^1.0.0-rc.3",
        "cron": "^1.8.2",
        "fastify": "^2.12.0",
        "fastify-plugin": "^1.6.0",
        "imgur-uploader": "^4.2.1",
        "jimp": "^0.8.5",
        "moment": "^2.24.0",
        "moment-timezone": "^0.5.27",
        "mongodb": "^3.5.3",
        "ts-node": "^8.8.1"
    },
    "devDependencies": {
        "eslint": "^6.8.0",
        "typescript": "^3.8.3"
    }
}